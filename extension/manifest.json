{
  "manifest_version": 2,

  "name": "mUrl",
  "description": "This extension get url for markdown",
  "version": "0.1",

  "browser_action": {
    "default_icon": "asset/murl_19.png",
    "default_popup": "popup.html",
    "default_title":"Get Url For Markdown"
  },
  "icons": {
    "16":"asset/murl_16.png",
    "48":"asset/murl_48.png",
    "128": "asset/murl_128.png"
  },
  "permissions": [
    "activeTab"
  ]
}